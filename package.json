{
  "name": "@wyattjoh/astro-cache",
  "version": "0.0.0-development",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wyattjoh/astro-cache.git"
  },
  "homepage": "https://github.com/wyattjoh/astro-cache#readme",
  "bugs": {
    "url": "https://github.com/wyattjoh/astro-cache/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "provenance": true
  },
  "exports": {
    ".": "./dist/index.js",
    "./cache.js": "./dist/cache.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "author": "Wyatt Johnson",
  "scripts": {
    "build": "tsc && bun build --packages external --target bun src/*.ts --outdir dist/",
    "test": "bun test --concurrent src/",
    "typecheck": "tsc -p . --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "prepare": "husky",
    "prepack": "bun run build",
    "release": "semantic-release"
  },
  "keywords": [
    "astro-integration",
    "cache",
    "swr",
    "withastro"
  ],
  "dependencies": {
    "flat-cache": "^6.1.20",
    "stale-while-revalidate-cache": "^3.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/bun": "^1.3.7",
    "astro": "^5.0.0",
    "husky": "^9.1.7",
    "semantic-release": "^25.0.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "astro": "^5.0.0"
  }
}
